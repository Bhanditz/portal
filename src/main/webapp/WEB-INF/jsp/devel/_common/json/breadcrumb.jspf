<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
"breadcrumb" : [
  <c:forEach items="${model.breadcrumbs}" var="crumb" varStatus="status">
    <c:if test="${crumb.showBreadCrumb}">
    {
      <c:if test="${!status.last}">"url" : "${crumb.breadCrumbUrl}",</c:if>
      "label" : "${crumb.display}"
    }<c:if test="${!status.last}">,</c:if>
    </c:if>
  </c:forEach>
],
