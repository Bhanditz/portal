<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<c:set var="view" value="table" />
<c:set var="query" value="" />
<c:if test="${param['view']}">
  <c:set var="view" value="${param['view']}" />
</c:if>
<c:if test="${param['query']}">
  <c:set var="query" value="${param['query']}" />
</c:if>
